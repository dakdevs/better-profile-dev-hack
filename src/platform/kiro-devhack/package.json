{
  "name": "better-profile-app",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "docker compose up -d && next dev",
    "dev:db": "docker compose up -d",
    "dev:app": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "db:stop": "docker compose down",
    "db:reset": "docker compose down -v && docker compose up -d && sleep 5 && pnpm db:push",
    "db:reset:script": "bash scripts/reset-db.sh",
    "db:setup": "bash scripts/setup-database.sh",
    "db:generate": "dotenvx run -f .env.local -- drizzle-kit generate",
    "db:migrate": "dotenvx run -f .env.local -- drizzle-kit migrate",
    "db:push": "dotenvx run -f .env.local -- drizzle-kit push",
    "db:studio": "dotenvx run -f .env.local -- drizzle-kit studio",
    "db:drop": "dotenvx run -f .env.local -- drizzle-kit drop",
    "test": "vitest",
    "test:run": "vitest run",
    "deploy": "bash scripts/deploy.sh",
    "monitor": "bash scripts/monitor-performance.sh",
    "monitor:continuous": "bash scripts/monitor-performance.sh continuous",
    "health-check": "bash scripts/monitor-performance.sh health-only"
  },
  "dependencies": {
    "@ai-sdk/openai": "^2.0.3",
    "@openrouter/ai-sdk-provider": "^1.0.0",
    "@t3-oss/env-nextjs": "^0.13.8",
    "ai": "^5.0.4",
    "better-auth": "^1.3.4",
    "clsx": "^2.1.1",
    "drizzle-orm": "^0.44.4",
    "lucide-react": "^0.536.0",
    "nanoid": "^5.1.5",
    "next": "15.4.5",
    "openai": "^5.12.0",
    "pg": "^8.16.3",
    "pgvector": "^0.2.0",
    "postgres": "^3.4.7",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "tailwind-merge": "^3.3.1",
    "zod": "^4.0.14"
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^1.48.4",
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.3.0",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/supertest": "^6.0.3",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "jsdom": "^26.1.0",
    "supertest": "^7.1.4",
    "tailwindcss": "^4",
    "typescript": "^5",
    "vitest": "^3.2.4"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "sharp",
      "unrs-resolver"
    ]
  },
  "packageManager": "pnpm@10.14.0"
}
